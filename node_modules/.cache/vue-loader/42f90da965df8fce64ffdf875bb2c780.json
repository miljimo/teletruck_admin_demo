o-col-1 {\n//   // flex-grow: 1;\n//   width: 30rem !important;\n//   @media screen and (min-width:1200px) {\n//     & {\n//       flex-grow: unset !important;\n//     }\n//   }\n// }\n\n@media screen and (min-width: 1201px) and (max-width: 1211px),\n  only screen and (min-width: 636px) and (max-width: 991px) {\n  #account-info-col-1 {\n    width: calc(100% - 12rem) !important;\n  }\n\n  // #account-manage-buttons {\n  //   width: 12rem !important;\n  //   flex-direction: column;\n\n  //   > button {\n  //     margin-right: 0 !important;\n  //     margin-bottom: 1rem;\n  //   }\n  // }\n}\n",{"version":3,"sources":["MemberView.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6KA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA","file":"MemberView.vue","sourceRoot":"src/views","sourcesContent":["<template>\n  <div id=\"page-user-view\">\n    <div id=\"user-data\">\n      <div class=\"centerx example-loading\">\n        <div class=\"fill-row-loading\">\n          <div id=\"loading-default\" class=\"vs-con-loading__container loading-example activeLoading\"></div>\n        </div>\n      </div>\n      <vx-card id=\"div-with-loading\" class=\"mb-base vs-con-loading__container\">\n        <div class=\"flex flex-wrap mb-5\">\n          <div class=\"w-full sm:w-1/2 md:w-1/2 lg:w-1/2 xl:w-1/2\">\n            <div class=\"mb-5\">\n              <a href=\"javascript:void()\" @click=\"$router.go(-1)\">‚Üê Back</a>\n            </div>\n          </div>\n          <div class=\"w-full sm:w-1/2 md:w-1/2 lg:w-1/2 xl:w-1/2 text-right\">\n            <vs-button color=\"primary\" type=\"filled\">Deactivate</vs-button>\n          </div>\n        </div>\n        <!-- Avatar -->\n        <div class=\"vx-row\">\n          <!-- Avatar Col -->\n          <div class=\"vx-col\" id=\"avatar-col\">\n            <div class=\"img-container mb-4\">\n              <img\n                v-if=\"user_data.img\"\n                :src=\"'https://d2meuqd6gjekk6.cloudfront.net/'+user_data.imgx250\"\n                class=\"logowidth\"\n              />\n            </div>\n          </div>\n\n          <!-- Information - Col 1 -->\n          <div class=\"vx-col flex-1\" id=\"account-info-col-1\">\n            <table>\n              <tr>\n                <td class=\"font-semibold\">Full Name</td>\n                <td>{{user_data.fullName}}</td>\n              </tr>\n              <tr>\n                <td class=\"font-semibold\">Email</td>\n                <td>\n                  <a :href=\"'mailto:'+user_data.email\" target=\"_blank\">{{user_data.email}}</a>\n                </td>\n              </tr>\n              <tr>\n                <td class=\"font-semibold\">Mobile Number</td>\n                <td>{{user_data.countryCode}}{{user_data.phoneNumber}}</td>\n              </tr>\n            </table>\n            <div class=\"vx-col w-full flex mt-5\" id=\"account-manage-buttons\">\n              <vs-button\n                type=\"border\"\n                color=\"danger\"\n                icon-pack=\"feather\"\n                icon=\"icon-trash\"\n                @click=\"confirmDeleteRecord\"\n              >Deactivate</vs-button>\n            </div>\n          </div>\n          <!-- /Information - Col 1 -->\n\n          <!-- Information - Col 2 -->\n          <div class=\"vx-col flex-1\" id=\"account-info-col-2\">\n            <table>\n              <tr v-if=\"user_data.churchId\">\n                <td class=\"font-semibold\">Church Name</td>\n                <td>{{user_data.churchId.name}}</td>\n              </tr>\n              <tr v-if=\"user_data.churchId\">\n                <td class=\"font-semibold\">Church Email</td>\n                <td>{{user_data.churchId.email}}</td>\n              </tr>\n            </table>\n          </div>\n          <!-- /Information - Col 2 -->\n        </div>\n      </vx-card>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      user_data: null,\n      user_not_found: false,\n    };\n  },\n  computed: {\n    userAddress() {\n      let str = \"\";\n      for (const field in this.user_data.location) {\n        str += `${field} `;\n      }\n      return str;\n    },\n  },\n  methods: {\n    openLoadingInDiv() {\n      this.$vs.loading({\n        container: \"#div-with-loading\",\n        scale: 0.6,\n      });\n    },\n    confirmDeleteRecord() {\n      this.$vs.dialog({\n        type: \"confirm\",\n        color: \"danger\",\n        title: \"Confirm Delete\",\n        text: `You are about to delete \"${this.user_data.username}\"`,\n        accept: this.deleteRecord,\n        acceptText: \"Delete\",\n      });\n    },\n    deleteRecord() {\n      /* Below two lines are just for demo purpose */\n      this.$router.push({ name: \"app-user-list\" });\n      this.showDeleteSuccess();\n\n      /* UnComment below lines for enabling true flow if deleting user */\n      // this.$store.dispatch(\"userManagement/removeRecord\", this.user_data.id)\n      //   .then(()   => { this.$router.push({name:'app-user-list'}); this.showDeleteSuccess() })\n      //   .catch(err => { console.error(err)       })\n    },\n    showDeleteSuccess() {\n      this.$vs.notify({\n        color: \"success\",\n        title: \"User Deleted\",\n        text: \"The selected user was successfully deleted\",\n      });\n    },\n  },\n  mounted() {\n    const userId = this.$route.params.userId;\n\n    this.$store\n      .dispatch(\"getMemberData\", userId)\n      .then((res) => {\n        // console.log(res.data);\n        this.user_data = res.data;\n      })\n      .catch((err) => {\n        if (err.response) {\n          if (err.response.status == 400) {\n            //        console.log(err.response.data);\n            this.$vs.notify({\n              title: \"Load Member Details\",\n              text: err.response.data,\n              color: \"danger\",\n            });\n          } else {\n            this.$vs.notify({\n              title: \"Load Member Details\",\n              text: \"Unable to Load member details!\",\n              color: \"danger\",\n            });\n          }\n        } else {\n          this.$vs.notify({\n            title: \"Load Member Details\",\n            text: \"Unable to Load member details!\",\n            color: \"danger\",\n          });\n        }\n        console.error(err);\n      });\n  },\n};\n</script>\n\n<style lang=\"scss\">\n#avatar-col {\n  width: 10rem;\n}\n\n#page-user-view {\n  table {\n    td {\n      vertical-align: top;\n      min-width: 140px;\n      padding-bottom: 0.8rem;\n      word-break: break-all;\n    }\n\n    &:not(.permissions-table) {\n      td {\n        @media screen and (max-width: 370px) {\n          display: block;\n        }\n      }\n    }\n  }\n}\n\n// #account-info-col-1 {\n//   // flex-grow: 1;\n//   width: 30rem !important;\n//   @media screen and (min-width:1200px) {\n//     & {\n//       flex-grow: unset !important;\n//     }\n//   }\n// }\n\n@media screen and (min-width: 1201px) and (max-width: 1211px),\n  only screen and (min-width: 636px) and (max-width: 991px) {\n  #account-info-col-1 {\n    width: calc(100% - 12rem) !important;\n  }\n\n  // #account-manage-buttons {\n  //   width: 12rem !important;\n  //   flex-direction: column;\n\n  //   > button {\n  //     margin-right: 0 !important;\n  //     margin-bottom: 1rem;\n  //   }\n  // }\n}\n</style>\n"]}]}