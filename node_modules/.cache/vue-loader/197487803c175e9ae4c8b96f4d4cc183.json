      this.$vs.notify({\n                title: \"Loag Blog Posts\",\n                text: err.response.data,\n                color: \"danger\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Loag Blog Posts\",\n                text: \"Unable to Load Blog Posts!\",\n                color: \"danger\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Loag Blog Posts\",\n              text: \"Unable to Load Blog Posts!\",\n              color: \"danger\",\n            });\n          }\n\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n        });\n    },\n  },\n  mounted() {\n    moment().format();\n    this.openLoadingInDiv();\n    this.getBlogPosts();\n  },\n};\n",{"version":3,"sources":["Blogs.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Blogs.vue","sourceRoot":"src/views","sourcesContent":["<template>\n  <div id=\"page-user-list\">\n    <vs-card class=\"p-3 pb-1\">\n      <div class=\"flex flex-wrap\">\n        <div class=\"w-full sm:w-1/2 md:w-1/2 lg:w-1/2 xl:w-1/2\">\n          <h5 class=\"m-0 pt-3\">All Blogs</h5>\n        </div>\n        <div class=\"w-full sm:w-1/2 md:w-1/2 lg:w-1/2 xl:w-1/2 text-right\">\n          <vs-button color=\"primary\" to=\"/create-blog\" type=\"filled\">Create Blog Post</vs-button>\n        </div>\n      </div>\n    </vs-card>\n    <vs-card class=\"p-3 pb-1 vs-con-loading__container\">\n      <vs-table id=\"div-with-loading\" search max-items=\"5\" pagination :data=\"blogs\">\n        <template slot=\"thead\">\n          <!-- <vs-th sort-key=\"email\">Id</vs-th> -->\n          <vs-th>Id</vs-th>\n          <vs-th>Date</vs-th>\n          <vs-th>Title</vs-th>\n          <vs-th>Body</vs-th>\n          <vs-th>Action</vs-th>\n        </template>\n\n        <template slot-scope=\"{data}\">\n          <vs-tr :key=\"index\" v-for=\"(tr, index) in data\">\n            <vs-td :data=\"tr._id\">{{ index + 1 }}</vs-td>\n            <vs-td>{{tr.createdAt | formatDate}}</vs-td>\n            <vs-td :data=\"tr.title\">\n              <a\n                @click=\"viewBlogPost(tr)\"\n                class=\"text-primary cursor-pointer\"\n              >{{ tr.title | capitalize}}</a>\n            </vs-td>\n            <vs-td :data=\"tr.body\">\n              <span :inner-html.prop=\"tr.body | truncate(70)\"></span>\n            </vs-td>\n            <vs-td>\n              <vs-button\n                @click=\"viewBlogPost(tr)\"\n                color=\"primary\"\n                type=\"filled\"\n                size=\"small\"\n                class=\"mr-2 mb-2\"\n              >View</vs-button>\n              <vs-button @click=\"editBlog(tr)\" class=\"mr-2 mb-2\" color=\"dark\" size=\"small\">Edit</vs-button>\n              <vs-button\n                @click=\"deletePost(tr._id)\"\n                class=\"mr-2 mb-2\"\n                type=\"border\"\n                color=\"dark\"\n                size=\"small\"\n              >Delete</vs-button>\n            </vs-td>\n          </vs-tr>\n        </template>\n      </vs-table>\n    </vs-card>\n\n    <vs-popup class=\"holamundo\" title=\"Confirm Delete\" :active.sync=\"confirmDelete\">\n      <p>Are you sure you want to delete Post?</p>\n      <div class=\"my-4\">\n        <vs-button class=\"mr-3\" color=\"black\" @click=\"confirmDeletePost\">Confirm</vs-button>\n        <vs-button type=\"border\" color=\"black\" @click=\"confirmDelete = false\">Close</vs-button>\n      </div>\n    </vs-popup>\n  </div>\n</template>\n\n<script>\nimport moment from \"moment\";\nimport vSelect from \"vue-select\";\nexport default {\n  components: {\n    vSelect,\n  },\n  data() {\n    return {\n      members: [],\n      confirmDelete: false,\n      blogs: [],\n      deleteID: \"\",\n    };\n  },\n  computed: {},\n  methods: {\n    openLoadingInDiv() {\n      this.$vs.loading({\n        container: \"#div-with-loading\",\n        scale: 0.6,\n      });\n    },\n    viewBlogPost(post) {\n      localStorage.setItem(\"postPrev\", JSON.stringify(post));\n      this.$router.push(`/view-blog/${post._id}`);\n    },\n    editBlog(post) {\n      localStorage.setItem(\"postPrev\", JSON.stringify(post));\n      this.$router.push(`/edit-blog/${post._id}`);\n    },\n    deletePost(id) {\n      this.confirmDelete = true;\n      this.deleteID = id;\n    },\n    confirmDeletePost() {\n      console.log(this.deleteID);\n      let blogID = this.deleteID;\n      this.openLoadingInDiv();\n      this.$store\n        .dispatch(\"deleteBlogPost\", blogID)\n        .then((resp) => {\n          this.$vs.notify({\n            title: \"Delete Blog Post\",\n            text: \"Post Deleted successful\",\n            color: \"success\",\n          });\n          this.confirmDelete = false;\n          this.getBlogPosts();\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n        })\n        .catch((err) => {\n          //        console.log(err);\n          if (err.response) {\n            if (err.response.status == 400) {\n              //   console.log(err.response.data);\n              this.$vs.notify({\n                title: \"Delete Blog Post\",\n                text: err.response.data,\n                color: \"danger\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Delete Blog Post\",\n                text: \"Unable to delete Blog Post!\",\n                color: \"danger\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Delete Blog Post\",\n              text: \"Unable to delete Blog Post!\",\n              color: \"danger\",\n            });\n          }\n\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n        });\n    },\n    getBlogPosts() {\n      this.$store\n        .dispatch(\"getBlogPosts\")\n        .then((resp) => {\n          //   console.log(resp.data.blog);\n          this.blogs = resp.data.blog;\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n        })\n        .catch((err) => {\n          //        console.log(err);\n          if (err.response) {\n            if (err.response.status == 400) {\n              //   console.log(err.response.data);\n              this.$vs.notify({\n                title: \"Loag Blog Posts\",\n                text: err.response.data,\n                color: \"danger\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Loag Blog Posts\",\n                text: \"Unable to Load Blog Posts!\",\n                color: \"danger\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Loag Blog Posts\",\n              text: \"Unable to Load Blog Posts!\",\n              color: \"danger\",\n            });\n          }\n\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n        });\n    },\n  },\n  mounted() {\n    moment().format();\n    this.openLoadingInDiv();\n    this.getBlogPosts();\n  },\n};\n</script>\n\n<style lang=\"scss\">\n#page-user-list {\n  .user-list-filters {\n    .vs__actions {\n      position: absolute;\n      right: 0;\n      top: 50%;\n      transform: translateY(-58%);\n    }\n  }\n}\n</style>\n"]}]}