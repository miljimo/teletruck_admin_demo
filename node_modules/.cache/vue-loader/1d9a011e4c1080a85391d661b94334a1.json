ourceRoot":"src/views","sourcesContent":["<template>\n  <div id=\"page-user-list\">\n    <vs-card class=\"p-3 pb-1\">\n      <div class=\"flex flex-wrap\">\n        <div class=\"w-full sm:w-1/2 md:w-1/2 lg:w-1/2 xl:w-1/2\">\n          <h5 class=\"m-0 pt-3\">Church Accounts</h5>\n        </div>\n        <div class=\"w-full sm:w-1/2 md:w-1/2 lg:w-1/2 xl:w-1/2 text-right\">\n          <vs-button color=\"primary\" to=\"/create-church-account\" type=\"filled\">Create Church Account</vs-button>\n        </div>\n      </div>\n    </vs-card>\n    <vs-card class=\"p-3 pb-1 vs-con-loading__container\">\n      <vs-table id=\"div-with-loading\" search max-items=\"5\" pagination :data=\"churches\">\n        <template slot=\"thead\">\n          <!-- <vs-th sort-key=\"email\">Id</vs-th> -->\n          <vs-th>Id</vs-th>\n          <vs-th>Date created</vs-th>\n          <vs-th>Church Name</vs-th>\n          <vs-th>Pastor</vs-th>\n          <vs-th>Mobile Number</vs-th>\n          <vs-th>Branches</vs-th>\n          <vs-th>Token</vs-th>\n          <vs-th>Location</vs-th>\n          <vs-th>Action</vs-th>\n        </template>\n\n        <template slot-scope=\"{data}\">\n          <vs-tr :key=\"index\" v-for=\"(tr, index) in data\">\n            <vs-td :data=\"tr.id\">{{ index + 1 }}</vs-td>\n            <vs-td :data=\"tr.createdAt\">{{ tr.createdAt | formatDate }}</vs-td>\n            <vs-td :data=\"tr.name\">{{ tr.name }}</vs-td>\n            <vs-td>\n              <span v-if=\"tr.HQID\">{{tr.HQID.pastor}}</span>\n            </vs-td>\n            <vs-td>\n              <span v-if=\"tr.HQID\">{{tr.HQID.phoneNumber}}</span>\n            </vs-td>\n            <vs-td :data=\"tr.branches\">{{ tr.branches.length }}</vs-td>\n            <vs-td :data=\"tr.publicToken\">\n              <vx-tooltip text=\"Copy Token\" position=\"bottom\">\n                <p\n                  v-clipboard:copy=\"tr.publicToken\"\n                  v-clipboard:success=\"onCopyToken\"\n                  v-clipboard:error=\"onErrorToken\"\n                  class=\"tokenpointer\"\n                >{{ tr.publicToken | truncate(7) }}</p>\n              </vx-tooltip>\n            </vs-td>\n            <vs-td>\n              <span v-if=\"tr.HQID\">{{tr.HQID.state}}, {{tr.HQID.country}}</span>\n            </vs-td>\n            <vs-td>\n              <vs-button\n                :to=\"'/view-church/'+ tr._id\"\n                color=\"primary\"\n                type=\"filled\"\n                size=\"small\"\n                class=\"mr-2 mb-2\"\n              >View</vs-button>\n              <!-- <vs-button\n                :to=\"'/edit-church/'+ tr._id\"\n                color=\"warning\"\n                type=\"filled\"\n                size=\"small\"\n                class=\"mr-2 mb-2\"\n              >Edit</vs-button>-->\n              <vs-button\n                :to=\"'/create-branch/'+ tr.publicToken\"\n                color=\"dark\"\n                size=\"small\"\n              >Create Branch</vs-button>\n            </vs-td>\n          </vs-tr>\n        </template>\n      </vs-table>\n    </vs-card>\n  </div>\n</template>\n\n<script>\nimport moment from \"moment\";\nimport vSelect from \"vue-select\";\nexport default {\n  components: {\n    vSelect,\n  },\n  data() {\n    return {\n      churches: [],\n    };\n  },\n\n  computed: {},\n  methods: {\n    openLoadingInDiv() {\n      this.$vs.loading({\n        container: \"#div-with-loading\",\n        scale: 0.6,\n      });\n    },\n    onCopyToken() {\n      this.$vs.notify({\n        text: \"Token Copied\",\n        color: \"success\",\n      });\n    },\n    onErrorToken() {\n      this.$vs.notify({\n        text: \"Error copying token\",\n        color: \"success\",\n      });\n    },\n  },\n  mounted() {\n    moment().format();\n    this.openLoadingInDiv();\n    this.$store\n      .dispatch(\"getChurches\")\n      .then((resp) => {\n        this.churches = resp.data;\n        console.log(this.churches);\n        this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n      })\n      .catch((err) => {\n        this.$vs.notify({\n          title: \"Load Churches\",\n          text: \"Unable to Load list of Churches!\",\n          color: \"danger\",\n        });\n        this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n      });\n  },\n  created() {},\n};\n</script>\n\n<style lang=\"scss\">\n#page-user-list {\n  .user-list-filters {\n    .vs__actions {\n      position: absolute;\n      right: 0;\n      top: 50%;\n      transform: translateY(-58%);\n    }\n  }\n}\n</style>\n"]}]}