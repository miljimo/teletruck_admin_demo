            <label>Blog Title</label>\n              <vs-input v-model=\"blog.title\" name=\"input1\" class=\"w-full mt-2\" />\n            </div>\n            <div class=\"pt-10\">\n              <label>Blog Post Content</label>\n              <!-- <vs-textarea v-model=\"blog.body\" name=\"input1\" class=\"w-full mt-2\" /> -->\n\n              <quill-editor\n                class=\"mt-2\"\n                height=\"500\"\n                ref=\"myQuillEditor\"\n                v-model=\"blog.body\"\n                :options=\"editorOption\"\n              />\n            </div>\n\n            <div class=\"pt-10\">\n              <vs-input\n                label=\"Blog Featured Image\"\n                accept=\"image/png, image/jpeg, image/jpg\"\n                @change=\"handleFileChange($event)\"\n                name=\"input1\"\n                type=\"file\"\n                class=\"w-full mt-0\"\n              />\n            </div>\n          </div>\n          <vs-button type=\"filled\" @click.prevent=\"submitForm\" class=\"mt-5 block\">Create Blog Post</vs-button>\n        </form>\n      </div>\n    </vs-card>\n  </div>\n</template>\n\n<script>\nimport vSelect from \"vue-select\";\nexport default {\n  components: {\n    \"v-select\": vSelect,\n  },\n  computed: {\n    editor() {\n      return this.$refs.myQuillEditor.quill;\n    },\n  },\n  data() {\n    return {\n      blog: {\n        title: \"\",\n        body: \"\",\n        img: \"\",\n      },\n      content: \"<h2>I am Example</h2>\",\n      editorOption: {\n        // Some Quill options...\n        theme: \"snow\",\n      },\n    };\n  },\n  mounted() {},\n  methods: {\n    openLoadingInDiv() {\n      this.$vs.loading({\n        container: \"#div-with-loading\",\n        scale: 0.6,\n      });\n    },\n    handleFileChange: function (event) {\n      this.blog.img = event.target.files[0];\n    },\n    submitForm() {\n      this.openLoadingInDiv();\n      let blogPost = this.blog;\n      this.$store\n        .dispatch(\"createBlogPost\", blogPost)\n        .then((resp) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n          this.$vs.notify({\n            title: \"Create Blog Post\",\n            text: \"New Post created successfully!\",\n            color: \"success\",\n          });\n          this.blog = {\n            title: \"\",\n            body: \"\",\n            img: \"\",\n          };\n        })\n        .catch((err) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n\n          if (err.response) {\n            if (err.response.status == 400) {\n              //        console.log(err.response.data);\n              this.$vs.notify({\n                title: \"Create Blog Post\",\n                text: err.response.data,\n                color: \"warning\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Create Blog Post\",\n                text: \"Unable to Create Blog Post, try again!\",\n                color: \"warning\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Create Blog Post\",\n              text: \"Unable to Create Blog Post, try again!\",\n              color: \"warning\",\n            });\n          }\n        });\n    },\n  },\n};\n</script>\n<style>\n[dir=\"ltr\"] .ql-editor {\n  min-height: 200px;\n}\n</style>"],"sourceRoot":"src/views"}]}