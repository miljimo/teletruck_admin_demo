       <vs-input\n                label-placeholder=\"Phone Number\"\n                v-model=\"admin.phoneNumber\"\n                name=\"input1\"\n                class=\"w-full mt-5\"\n              />\n            </div>\n            <div class=\"vx-col w-1/2 pt-3\">\n              <vs-input\n                label-placeholder=\"Email\"\n                v-model=\"admin.email\"\n                name=\"input1\"\n                type=\"email\"\n                class=\"w-full mt-5\"\n              />\n            </div>\n            <div class=\"vx-col w-1/2 pt-3\">\n              <vs-input\n                label=\"Profile Image\"\n                accept=\"image/png, image/jpeg, image/jpg\"\n                @change=\"handleFileChange($event)\"\n                name=\"input1\"\n                type=\"file\"\n                class=\"w-full mt-0\"\n              />\n            </div>\n\n            <div class=\"vx-col w-1/2 pt-3\">\n              <vs-input\n                label-placeholder=\"Password\"\n                v-model=\"admin.password\"\n                type=\"password\"\n                name=\"input1\"\n                class=\"w-full mt-5\"\n              />\n            </div>\n\n            <div class=\"vx-col w-1/2 pt-3\">\n              <vs-input\n                label-placeholder=\"Confirm Password\"\n                v-model=\"admin.confirmPassword\"\n                name=\"input1\"\n                type=\"password\"\n                class=\"w-full mt-5\"\n              />\n            </div>\n          </div>\n          <vs-button type=\"filled\" @click.prevent=\"submitForm\" class=\"mt-5 block\">Create Account</vs-button>\n        </form>\n      </div>\n    </vs-card>\n  </div>\n</template>\n\n<script>\nimport vSelect from \"vue-select\";\nexport default {\n  components: {\n    \"v-select\": vSelect,\n  },\n  data() {\n    return {\n      type: [],\n      states: [],\n      adminType: [\"admin\", \"accounting\", \"customerCare\"],\n      admin: {\n        firstName: \"\",\n        type: \"\",\n        lastName: \"\",\n        countryCode: \"\",\n        phoneNumber: \"\",\n        email: \"\",\n        img: \"\",\n        password: \"\",\n        confirmPassword: \"\",\n      },\n    };\n  },\n  mounted() {},\n  methods: {\n    openLoadingInDiv() {\n      this.$vs.loading({\n        container: \"#div-with-loading\",\n        scale: 0.6,\n      });\n    },\n    handleFileChange: function (event) {\n      console.log(event.target.files[0]);\n      this.admin.img = event.target.files[0];\n    },\n    submitForm() {\n      // this.admin.State = this.admin.State.name;\n      // this.admin.countryId = this.admin.countryId._id;\n      this.openLoadingInDiv();\n      let admin = this.admin;\n      console.log(admin);\n      this.$store\n        .dispatch(\"createadmin\", admin)\n        .then((resp) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n          this.$vs.notify({\n            title: \"Create Admin Account\",\n            text: \"Admin Account created successfully!\",\n            color: \"success\",\n          });\n          this.admin = {\n            firstName: \"\",\n            type: \"\",\n            lastName: \"\",\n            countryCode: \"\",\n            phoneNumber: \"\",\n            email: \"\",\n            img: \"\",\n            password: \"\",\n            confirmPassword: \"\",\n          };\n          //   console.log(resp);\n        })\n        .catch((err) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n\n          if (err.response) {\n            if (err.response.status == 400) {\n              //        console.log(err.response.data);\n              this.$vs.notify({\n                title: \"Create admin Account\",\n                text: err.response.data,\n                color: \"warning\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Create admin Account\",\n                text: \"Unable to create admin account, try again!\",\n                color: \"warning\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Create admin Account\",\n              text: \"Unable to create admin account, try again!\",\n              color: \"warning\",\n            });\n          }\n        });\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}