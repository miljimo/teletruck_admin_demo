s-input\n                label=\"Logo\"\n                accept=\"image/png, image/jpeg, image/jpg\"\n                @change=\"handleFileChange($event)\"\n                name=\"input1\"\n                type=\"file\"\n                class=\"w-full mt-0\"\n              />\n            </div>\n\n            <!-- New Details -->\n            <div class=\"vx-col w-1/2 pt-3\">\n              <vs-input\n                label-placeholder=\"Mobile Domain\"\n                v-model=\"church.mobileDomain\"\n                name=\"input1\"\n                class=\"w-full mt-5\"\n              />\n            </div>\n            <div class=\"vx-col w-1/2 pt-3\">\n              <vs-input\n                label-placeholder=\"Android URL\"\n                v-model=\"church.androidUrl\"\n                name=\"input1\"\n                class=\"w-full mt-5\"\n              />\n            </div>\n            <div class=\"vx-col w-1/2 pt-3\">\n              <vs-input\n                label-placeholder=\"IOS URL\"\n                v-model=\"church.iosUrl\"\n                name=\"input1\"\n                class=\"w-full mt-5\"\n              />\n            </div>\n            <div class=\"vx-col w-1/2 pt-3\">\n              <vs-input\n                label-placeholder=\"Setup Fee in Naira\"\n                v-model=\"church.setupFeeNaira\"\n                name=\"input1\"\n                class=\"w-full mt-5\"\n              />\n            </div>\n            <div class=\"vx-col w-1/2 pt-3\">\n              <vs-input\n                label-placeholder=\"Setup Fee in Dollar\"\n                v-model=\"church.setupFeeDollar\"\n                name=\"input1\"\n                class=\"w-full mt-5\"\n              />\n            </div>\n            <div class=\"vx-col w-1/2 pt-3\">\n              <vs-input\n                label-placeholder=\"Subscription Fee in Naira\"\n                v-model=\"church.subscriptionFeeNaira\"\n                name=\"input1\"\n                class=