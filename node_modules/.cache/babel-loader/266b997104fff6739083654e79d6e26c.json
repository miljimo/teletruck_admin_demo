 <vs-list>\r\n            <vs-list-item\r\n              v-for=\"(transaction, index) in dashboardData.transactions\"\r\n              :key=\"index\"\r\n              class=\"px-0 my-3\"\r\n              v-if=\"index <= 5\"\r\n              :title=\"`${transaction.meta.user} - ${transaction.meta.description}`\"\r\n              :subtitle=\"\r\n                moment\r\n                  .utc(new Date(transaction.created_at))\r\n                  .format('dddd, MMM Do YY')\r\n              \"\r\n            >\r\n              <p class=\"text-gray text-small\">\r\n                {{ transaction.amount | currency(\"₦\", 0) }}\r\n              </p>\r\n            </vs-list-item>\r\n          </vs-list>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-md-7\">\r\n        <div class=\"row\">\r\n          <div class=\"col-md-6\">\r\n            <vs-card>\r\n              <div class=\"p-3\">\r\n                <div class=\"row\">\r\n                  <div class=\"col-9\">\r\n                    <h4 class=\"font-bold\">{{ dashboardData.trips }}</h4>\r\n                    <p class=\"mt-3\">Total Trips</p>\r\n                  </div>\r\n                  <div class=\"col-3\">\r\n                    <h4>\r\n                      <feather-icon class=\"mr-1\" icon=\"TruckIcon\" />\r\n                    </h4>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </vs-card>\r\n          </div>\r\n          <div class=\"col-md-6\">\r\n            <vs-card>\r\n              <div class=\"p-3\">\r\n                <div class=\"row\">\r\n                  <div class=\"col-9\">\r\n                    <h4 class=\"font-bold\">{{ dashboardData.materials }}</h4>\r\n                    <p class=\"mt-3\">Total Materials</p>\r\n                  </div>\r\n                  <div class=\"col-3\">\r\n                    <h4>\r\n                      <feather-icon class=\"mr-1\" icon=\"BoxIcon\" />\r\n                    </h4>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </vs-card>\r\n          </div>\r\n\r\n          <div class=\"col-md-6\">\r\n            <vs-card>\r\n              <div class=\"p-3\">\r\n                <div class=\"row\">\r\n                  <div class=\"col-9\">\r\n                    <h4 class=\"font-bold\">\r\n                      {{ dashboardData.payin | currency(\"₦\") }}\r\n                    </h4>\r\n                    <p class=\"mt-3\">Total Payin</p>\r\n                  </div>\r\n                  <div class=\"col-3\">\r\n                    <h4>\r\n                      <feather-icon class=\"mr-1\" icon=\"TrendingUpIcon\" />\r\n                    </h4>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </vs-card>\r\n          </div>\r\n          <div class=\"col-md-6\">\r\n            <vs-card>\r\n              <div class=\"p-3\">\r\n                <div class=\"row\">\r\n                  <div class=\"col-9\">\r\n                    <h4 class=\"font-bold\">\r\n                      {{ dashboardData.payout | currency(\"₦\") }}\r\n                    </h4>\r\n                    <p class=\"mt-3\">Total Payout</p>\r\n                  </div>\r\n                  <div class=\"col-3\">\r\n                    <h4>\r\n                      <feather-icon class=\"mr-1\" icon=\"TrendingDownIcon\" />\r\n                    </h4>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </vs-card>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"mt-3\">\r\n          <vs-list-item title=\"Recent Orders\"> </vs-list-item>\r\n          <vs-list>\r\n            <vs-list-item\r\n              v-for=\"(order, index) in dashboardData.recent_orders\"\r\n              :key=\"index\"\r\n              class=\"px-0 my-3\"\r\n              :title=\"`${order.material.name}, Total amount - ${order.total_amount} `\"\r\n              :subtitle=\"`${order.order_s