\n            <vs-td>\n              <a :href=\"'mailto:'+tr.email\" target=\"_blank\">{{tr.email}}</a>\n            </vs-td>\n            <vs-td>{{tr.state}}, {{tr.country}}</vs-td>\n            <vs-td>\n              <vs-button\n                @click=\"$router.push({\n                  path: '/view-branch',\n                  query: { branchId: tr._id, branchName: tr.name },\n                })\"\n                color=\"primary\"\n                type=\"filled\"\n                size=\"small\"\n                class=\"mr-2 mb-2\"\n              >View Branch</vs-button>\n            </vs-td>\n          </vs-tr>\n        </template>\n      </vs-table>\n    </vs-card>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      churchBranch: [],\n    };\n  },\n  methods: {\n    openLoadingInDiv() {\n      this.$vs.loading({\n        container: \"#div-with-loading\",\n        scale: 0.6,\n      });\n    },\n    getChurchBranch() {\n      const churchId = this.$route.query.churchId;\n      this.$store\n        .dispatch(\"getChurchBranch\", churchId)\n        .then((resp) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n          //        console.log(resp.data);\n          this.churchBranch = resp.data;\n        })\n        .catch((err) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n          //        console.log(err.response);\n\n          if (err.response) {\n            if (err.response.status == 400) {\n              //        console.log(err.response.data);\n              this.$vs.notify({\n                title: \"Church Branch\",\n                text: err.response.data,\n                color: \"warning\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Church Branch\",\n                text: \"Unable to load church branch, try again!\",\n                color: \"warning\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Church Account\",\n              text: \"Unable to load church branch, try again!\",\n              color: \"warning\",\n            });\n          }\n        });\n    },\n  },\n  mounted() {\n    this.openLoadingInDiv();\n    this.getChurchBranch();\n  },\n};\n</script>"],"sourceRoot":"src/views"}]}