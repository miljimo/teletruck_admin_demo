n\n                      @click=\"saveImageUpload()\"\n                      color=\"warning\"\n                      type=\"filled\"\n                      class=\"mr-2 mb-2\"\n                    >Save Image</vs-button>\n                    <vs-button @click=\"imgpreview = false\" color=\"danger\" type=\"flat\">Cancel</vs-button>\n                  </div>\n                  <div v-else>\n                    <input id=\"fileUpload\" @change=\"handleFileChange($event)\" type=\"file\" hidden />\n                    <vs-button\n                      @click=\"changeImage()\"\n                      color=\"dark\"\n                      type=\"filled\"\n                      class=\"mr-2 mb-2\"\n                    >Change Image</vs-button>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n          <div class=\"vx-col w-full lg:w-3/4 xl:w-3/4\">\n            <form class=\"mt-4\">\n              <div class=\"vx-row\">\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <vs-input\n                    label-placeholder=\"Church Name\"\n                    v-model=\"church.name\"\n                    name=\"input1\"\n                    class=\"w-full mt-5\"\n                  />\n                </div>\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <label>Country</label>\n                  <v-select\n                    @input=\"changeCountry\"\n                    class=\"mt-1\"\n                    v-model=\"church.countryId\"\n                    label=\"name\"\n                    :options=\"countries\"\n                  />\n                </div>\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <label>State</label>\n                  <v-select class=\"mt-1\" v-model=\"church.state\" label=\"name\" :options=\"states\" />\n                </div>\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <vs-input\n                    label-placeholder=\"Region\"\n                    v-model=\"church.region\"\n                    name=\"input1\"\n                    class=\"w-full mt-5\"\n                  />\n                </div>\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <vs-input\n                    label-placeholder=\"Country Code\"\n                    v-model=\"church.countryCode\"\n                    name=\"input1\"\n                    class=\"w-full mt-5\"\n                  />\n                </div>\n\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <vs-input\n                    label-placeholder=\"Phone Number\"\n                    v-model=\"church.phoneNumber\"\n                    name=\"input1\"\n                    class=\"w-full mt-5\"\n                  />\n                </div>\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <vs-input\n                    label-placeholder=\"Email\"\n                    v-model=\"church.email\"\n                    name=\"input1\"\n                    class=\"w-full mt-5\"\n                  />\n                </div>\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <vs-input\n                    label-placeholder=\"Stream URL\"\n                    v-model=\"church.streamURL\"\n                    name=\"input1\"\n                    class=\"w-full mt-5\"\n                  />\n                </div>\n\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <vs-input\n                    label-placeholder=\"Contact Phone number\"\n                    v-model=\"church.contactPhoneNumber\"\n                    name=\"input1\"\n                    class=\"w-full mt-5\"\n                  />\n                </div>\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <vs-input\n                    label-placeholder=\"Address\"\n                    v-model=\"church.address\"\n                    name=\"input1\"\n                    class=\"w-full mt-5\"\n                  />\n                </div>\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <vs-input\n                    label-placeholder=\"Zone\"\n                    v-model=\"church.zone\"\n                    name=\"input1\"\n                    class=\"w-full mt-5\"\n                  />\n                </div>\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <vs-input\n                    label-placeholder=\"Pastor\"\n                    v-model=\"church.pastor\"\n                    name=\"input1\"\n                    class=\"w-full mt-5\"\n                  />\n                </div>\n                <!-- New Details -->\n\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <vs-input\n                    label-placeholder=\"Setup Fee in Naira\"\n                    v-model=\"church.setupFeeNaira\"\n                    name=\"input1\"\n                    class=\"w-full mt-5\"\n                  />\n                </div>\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <vs-input\n                    label-placeholder=\"Setup Fee in Dollar\"\n                    v-model=\"church.setupFeeDollar\"\n                    name=\"input1\"\n                    class=\"w-full mt-5\"\n                  />\n                </div>\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <vs-input\n                    label-placeholder=\"Subscription Fee in Naira\"\n                    v-model=\"church.subscriptionFeeNaira\"\n                    name=\"input1\"\n                    class=\"w-full mt-5\"\n                  />\n                </div>\n                <div class=\"vx-col w-1/2 pt-3\">\n                  <vs-input\n                    label-placeholder=\"Subscription Fee in Dollar\"\n                    v-model=\"church.subscriptionFeeDollar\"\n                    name=\"input1\"\n                    class=\"w-full mt-5\"\n                  />\n                </div>\n              </div>\n              <vs-button type=\"filled\" @click.prevent=\"submitForm\" class=\"mt-5 block\">Edit Account</vs-button>\n            </form>\n          </div>\n        </div>\n      </div>\n    </vs-card>\n  </div>\n</template>\n\n<script>\nimport vSelect from \"vue-select\";\nexport default {\n  components: {\n    \"v-select\": vSelect,\n  },\n  data() {\n    return {\n      countries: [],\n      states: [],\n      churchInfo: {},\n      imgpreview: false,\n      churchimg: \"\",\n      imageDataUpload: \"\",\n      previewUrl: \"\",\n      church: {\n        name: \"\",\n        state: \"\",\n        region: \"\",\n        countryCode: \"\",\n        phoneNumber: \"\",\n        email: \"\",\n        countryId: \"\",\n        address: \"\",\n        zone: \"\",\n        pastor: \"\",\n        streamURL: \"\",\n        // New Fields\n        contactPhoneNumber: \"\",\n        setupFeeNaira: \"\",\n        setupFeeDollar: \"\",\n        subscriptionFeeDollar: \"\",\n        subscriptionFeeNaira: \"\",\n      },\n    };\n  },\n  mounted() {\n    this.openLoadingInDiv();\n    this.getChurchDetails();\n    this.$store\n      .dispatch(\"getCountries\")\n      .then((resp) => {\n        this.countries = resp.data;\n        this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n      })\n      .catch((err) => {\n        this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n      });\n  },\n  methods: {\n    openLoadingInDiv() {\n      this.$vs.loading({\n        container: \"#div-with-loading\",\n        scale: 0.6,\n      });\n    },\n    handleFileChange: function (event) {\n      //   console.log(event.target.files[0]);\n      const file = event.target.files[0];\n      this.imageDataUpload = file;\n      this.previewUrl = URL.createObjectURL(file);\n      this.imgpreview = true;\n      //   this.church.Logo = event.target.files[0];\n    },\n    saveImageUpload() {\n      this.openLoadingInDiv();\n      let churchData = {\n        logo: this.imageDataUpload,\n        churchId: this.$route.query._id,\n      };\n      this.$store\n        .dispatch(\"updateChurchLogo\", churchData)\n        .then((resp) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n          this.$vs.notify({\n            title: \"Church Logo\",\n            text: \"Church Logo updated successfully!\",\n            color: \"success\",\n          });\n          this.getChurchDetails();\n          this.imgpreview = false;\n          //   this.churchInfo = resp.data;\n        })\n        .catch((err) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n          //   console.log(err.response);\n          if (err.response) {\n            if (err.response.status == 400) {\n              // console.log(err.response.data);\n              this.$vs.notify({\n                title: \"Church Logo\",\n                text: err.response.data,\n                color: \"warning\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Church Logo\",\n                text: \"Unable to edit church logo, try again!\",\n                color: \"warning\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Church Account\",\n              text: \"Unable to edit church logo, try again!\",\n              color: \"warning\",\n            });\n          }\n        });\n    },\n    getChurchDetails() {\n      this.openLoadingInDiv();\n      const churchId = this.$route.params.id;\n      this.$store\n        .dispatch(\"getBranchDetails\", churchId)\n        .then((resp) => {\n          // this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n          this.churchInfo = resp.data;\n          this.church.name = this.churchInfo.name;\n          this.church.countryId = this.churchInfo.countryId;\n          this.church.state = this.churchInfo.state;\n          this.church.region = this.churchInfo.region;\n          this.church.countryCode = this.churchInfo.countryCode;\n          this.church.phoneNumber = this.churchInfo.phoneNumber;\n          this.church.email = this.churchInfo.email;\n\n          this.church.address = this.churchInfo.address;\n          this.church.streamURL = this.churchInfo.streamURL;\n\n          this.church.zone = this.churchInfo.zone;\n          this.church.pastor = this.churchInfo.pastor;\n          if (this.churchInfo.contactPhoneNumber) {\n            this.church.contactPhoneNumber = this.churchInfo.contactPhoneNumber;\n          }\n\n          this.church.setupFeeNaira = this.churchInfo.setupFeeNaira;\n          this.church.setupFeeDollar = this.churchInfo.setupFeeDollar;\n          this.church.subscriptionFeeDollar = this.churchInfo.subscriptionFeeDollar;\n          this.church.subscriptionFeeNaira = this.churchInfo.subscriptionFeeNaira;\n\n          this.churchimg =\n            \"https://d2meuqd6gjekk6.cloudfront.net/\" + this.churchInfo.img;\n          //    this.church.transCost = this.churchInfo.pastor;\n\n          this.$store\n            .dispatch(\"getState\", this.church.countryId)\n            .then((resp) => {\n              this.states = resp.data;\n              this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n            })\n            .catch((err) => {\n              this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n            });\n        })\n        .catch((err) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n          // console.log(err.response);\n\n          if (err.response) {\n            if (err.response.status == 400) {\n              // console.log(err.response.data);\n              this.$vs.notify({\n                title: \"Church Account\",\n                text: err.response.data,\n                color: \"warning\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Church Account\",\n                text: \"Unable to load church details, try again!\",\n                color: \"warning\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Church Account\",\n              text: \"Unable to load church details, try again!\",\n              color: \"warning\",\n            });\n          }\n        });\n    },\n    changeCountry(value) {\n      this.openLoadingInDiv();\n      let countryId = value._id;\n      this.$store\n        .dispatch(\"getState\", countryId)\n        .then((resp) => {\n          this.states = resp.data;\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n        })\n        .catch((err) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n        });\n    },\n    changeImage() {\n      document.getElementById(\"fileUpload\").click();\n      //   console.log(event.target.files[0]);\n      //   this.church.Logo = event.target.files[0];\n    },\n    submitForm() {\n      // this.church.state = this.church.state.name;\n      // this.church.countryId = this.church.countryId._id;\n      this.openLoadingInDiv();\n\n      this.church.state = this.church.state.name;\n\n      let editChurchData = {\n        church: this.church,\n        churchId: this.$route.query._id,\n      };\n      this.$store\n        .dispatch(\"editChurch\", editChurchData)\n        .then((resp) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n          this.$vs.notify({\n            title: \"Edit Church Account\",\n            text: \"Church Account editted successfully!\",\n            color: \"success\",\n          });\n          //   console.log(resp);\n        })\n        .catch((err) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n\n          if (err.response) {\n            if (err.response.status == 400) {\n              // console.log(err.response.data);\n              this.$vs.notify({\n                title: \"Create Church Account\",\n                text: err.response.data,\n                color: \"warning\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Edit Church Account\",\n                text: \"Unable to edit Church account, try again!\",\n                color: \"warning\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Edit Church Account\",\n              text: \"Unable to edit Church account, try again!\",\n              color: \"warning\",\n            });\n          }\n        });\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}