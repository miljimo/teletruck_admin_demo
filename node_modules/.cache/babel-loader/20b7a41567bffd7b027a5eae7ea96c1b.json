n\n                  <!-- Add font size dropdown -->\n                  <select class=\"ql-size\">\n                    <option value=\"small\" />\n                    <!-- Note a missing, thus falsy value, is used to reset to default -->\n                    <option selected />\n                    <option value=\"large\" />\n                    <option value=\"huge\" />\n                  </select>\n\n                  <select class=\"ql-font\">\n                    <option selected=\"selected\" />\n                    <option value=\"serif\" />\n                    <option value=\"monospace\" />\n                  </select>\n\n                  <!-- Add subscript and superscript buttons -->\n                  <button class=\"ql-script\" value=\"sub\" />\n                  <button class=\"ql-script\" value=\"super\" />\n\n                  <!-- You can also add your own -->\n                </div>\n              </quill-editor>\n            </div>\n\n            <div class=\"mb-5\">\n              <h4>Body</h4>\n              <!-- synopsis -->\n              <quill-editor class=\"mt-5 smEditor\" v-model=\"body\" />\n            </div>\n          </div>\n        </vs-card>\n      </div>\n      <div class=\"col-md-4\">\n        <vs-card>\n          <div class=\"p-3\">\n            <div class=\"mb-5\">\n              <p class=\"lead\">Tag</p>\n\n              <input-tag\n                v-model=\"tags\"\n                placeholder=\"Add Tags\"\n                :add-tag-on-blur=\"true\"\n                style=\"border-radius: 4px; margin-top: 4px\"\n              ></input-tag>\n            </div>\n            <!-- <div class=\"mb-5\">\n              <p class=\"lead\">Album</p>\n              <vs-input class=\"w-full mt-2\" placeholder=\"\" />\n            </div> -->\n            <div class=\"mb-5\">\n              <p class=\"lead\">Maternal Status</p>\n\n              <vs-select class=\"mb-4 w-full\" v-model=\"maternal_id\">\n                <vs-select-item\n                  :key=\"index\"\n                  :value=\"item.id\"\n                  :text=\"item.name\"\n                  v-for=\"(item, index) in maternalStatus\"\n                />\n              </vs-select>\n            </div>\n            <div class=\"mb-5\">\n              <img\n                :src=\"image\"\n                class=\"mb-4\"\n                alt=\"\"\n                style=\"width: 100%; border-radius: 23px\"\n              />\n              <p class=\"lead\">Banner image</p>\n\n              <vs-input\n                @change=\"addImage($event)\"\n                class=\"w-full mt-2\"\n                placeholder=\"\"\n                type=\"file\"\n              />\n            </div>\n          </div>\n        </vs-card>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\n// eslint-disable-next-line\nimport \"quill/dist/quill.core.css\";\n// eslint-disable-next-line\nimport \"quill/dist/quill.snow.css\";\n// eslint-disable-next-line\nimport \"quill/dist/quill.bubble.css\";\nimport { quillEditor } from \"vue-quill-editor\";\nexport default {\n  components: {\n    quillEditor,\n  },\n  computed: {\n    loading() {\n      return this.$store.getters.pgLoading;\n    },\n  },\n  data() {\n    return {\n      name: \"\",\n      maternal_id: \"\",\n      tags: \"\",\n      // album_id: \"\",\n      synopsis: \"\",\n      body: \"\",\n      image: \"\",\n      editorOption: {\n        modules: {\n          toolbar: \"#toolbar\",\n        },\n      },\n      newOption: {\n        modules: {\n          toolbar: \"#nbar\",\n        },\n      },\n      newOptionMother: {\n        modules: {\n          toolbar: \"#newOptionMother\",\n        },\n      },\n\n      editorOption2: {\n        modules: {\n          toolbar: \"#toolbar\",\n        },\n      },\n      synopsis: \"\",\n      body: \"\",\n      content: ``,\n      maternalStatus: [],\n    };\n  },\n  mounted() {\n    this.getBlog();\n    this.getStatus();\n  },\n  methods: {\n    getBlog() {\n      this.$store.commit(\"pgLoading\", true);\n      let id = this.$route.params.id;\n\n      this.$store\n        .dispatch(\"getBlog\", id)\n        .then((resp) => {\n          this.content = resp.data.data;\n\n          this.name = this.content.name;\n          this.maternal_id = this.content.maternal_id;\n          // this.tags = this.content\n\n          this.synopsis = this.content.synopsis;\n          this.body = this.content.body;\n          this.image = this.content.image;\n\n          this.$store.commit(\"pgLoading\", false);\n        })\n        .catch((err) => {\n          this.$store.commit(\"pgLoading\", false);\n        });\n    },\n    getStatus() {\n      this.$store.commit(\"pgLoading\", true);\n      // this.$vs.loading();\n      this.$store\n        .dispatch(\"getMaternal\")\n        .then((resp) => {\n          this.maternalStatus = resp.data;\n          this.$store.commit(\"pgLoading\", false);\n        })\n        .catch((err) => {\n          this.$store.commit(\"pgLoading\", false);\n        });\n    },\n    getBase64(file) {\n      var reader = new FileReader();\n      reader.readAsDataURL(file);\n      reader.onload = () => {\n        this.image = reader.result;\n      };\n      reader.onerror = (error) => {\n        return false;\n      };\n    },\n    addImage(event) {\n      this.getBase64(event.target.files[0]);\n    },\n    saveContent() {\n      this.$vs.loading();\n      let blog = {\n        blog_id: parseInt(this.$route.params.id),\n        name: this.name,\n        maternal_id: this.maternal_id.toString(),\n        tags: this.tags,\n        album_id: \"\",\n        synopsis: this.synopsis,\n        body: this.body,\n        image: this.image,\n      };\n      this.$store\n        .dispatch(\"editBlog\", blog)\n        .then((resp) => {\n          if (resp.data.status) {\n            this.$vs.loading.close();\n            this.$vs.notify({\n              title: \"Edit Blog\",\n              text: \"New Blog added successful\",\n              color: \"success\",\n              icon: \"verified_user\",\n              position: \"bottom-center\",\n            });\n          }\n        })\n        .catch((err) => {\n          this.$vs.loading.close();\n          if (err.response) {\n            this.$vs.notify({\n              title: \"Edit Blog\",\n              text: err.response.data.message,\n              color: \"warning\",\n              icon: \"error\",\n              position: \"bottom-center\",\n            });\n          } else {\n            this.$vs.notify({\n              title: \"Edit Blog\",\n              text: \"Unable to edit blog\",\n              color: \"dark\",\n              icon: \"error\",\n              position: \"bottom-center\",\n            });\n          }\n        });\n    },\n  },\n};\n</script>"],"sourceRoot":"src/views"}]}