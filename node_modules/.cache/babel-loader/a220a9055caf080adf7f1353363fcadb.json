tr._id\">{{ index + 1 }}</vs-td>\n            <vs-td>{{tr.createdAt | formatDate}}</vs-td>\n            <vs-td>\n              <a class=\"text-primary cursor-pointer\">{{ tr.churchName | capitalize}}</a>\n            </vs-td>\n\n            <vs-td>\n              <a :href=\"`mailto:${tr.email}`\" target=\"_blank\">{{tr.email}}</a>\n            </vs-td>\n\n            <vs-td>{{tr.phoneNumber}}</vs-td>\n            <vs-td>{{tr.name}}</vs-td>\n          </vs-tr>\n        </template>\n      </vs-table>\n    </vs-card>\n\n    <vs-popup class=\"holamundo\" :title=\"previewRequest.name\" :active.sync=\"viewRequestSign\">\n      <vs-list>\n        <vs-list-item title=\"Church Name\" :subtitle=\"previewRequest.name\"></vs-list-item>\n        <vs-list-item\n          title=\"Location\"\n          :subtitle=\"`${previewRequest.state}, ${previewRequest.city}`\"\n        ></vs-list-item>\n        <vs-list-item title=\"Address\" :subtitle=\"previewRequest.address\"></vs-list-item>\n        <vs-list-item title=\"Location Type\" :subtitle=\"previewRequest.locationType | capitalize\"></vs-list-item>\n        <vs-list-item title=\"Email address\" :subtitle=\"previewRequest.email\"></vs-list-item>\n        <vs-list-header title=\"Contact Information\"></vs-list-header>\n        <vs-list-item\n          title=\"Contact Name\"\n          :subtitle=\"`${previewRequest.title} ${previewRequest.contactName}`\"\n        ></vs-list-item>\n        <vs-list-item title=\"Contact Phone\" :subtitle=\"previewRequest.contactPhoneNumber\"></vs-list-item>\n        <vs-list-header title=\"Other Information\"></vs-list-header>\n        <vs-list-item title=\"App Type\" :subtitle=\"previewRequest.type\"></vs-list-item>\n        <vs-list-item title=\"Postal Code\" :subtitle=\"previewRequest.postCode\"></vs-list-item>\n        <vs-list-item title=\"App Brand\" :subtitle=\"previewRequest.brand\"></vs-list-item>\n        <vs-list-item title=\"Mobile App Type\" v-if=\"previewRequest.android\" subtitle=\"Android\"></vs-list-item>\n        <vs-list-item title=\"Mobile App Type\" v-if=\"previewRequest.ios\" subtitle=\"IOS\"></vs-list-item>\n      </vs-list>\n      <div class=\"my-4\"></div>\n    </vs-popup>\n  </div>\n</template>\n\n<script>\nimport moment from \"moment\";\nimport vSelect from \"vue-select\";\nexport default {\n  components: {\n    vSelect,\n  },\n  data() {\n    return {\n      viewRequestSign: false,\n      blogs: [],\n      requests: [],\n      deleteID: \"\",\n      previewRequest: {},\n    };\n  },\n  computed: {},\n  methods: {\n    openLoadingInDiv() {\n      this.$vs.loading({\n        container: \"#div-with-loading\",\n        scale: 0.6,\n      });\n    },\n    getReferrals() {\n      this.$store\n        .dispatch(\"getReferrals\")\n        .then((resp) => {\n          // console.log(resp.data);`\n          this.requests = resp.data.request;\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n        })\n        .catch((err) => {\n          //        console.log(err);\n          if (err.response) {\n            if (err.response.status == 400) {\n              //   console.log(err.response.data);\n              this.$vs.notify({\n                title: \"Load Referrals\",\n                text: err.response.data,\n                color: \"danger\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Load Referrals\",\n                text: \"Unable to Load Referrals\",\n                color: \"danger\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Load Referrals\",\n              text: \"Unable to Load Referrals\",\n              color: \"danger\",\n            });\n          }\n\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n        });\n    },\n    viewRequest(request) {\n      this.viewRequestSign = true;\n      console.log(request);\n      this.previewRequest = request;\n    },\n  },\n  mounted() {\n    moment().format();\n    this.openLoadingInDiv();\n    this.getReferrals();\n  },\n};\n</script>\n\n<style lang=\"scss\">\n#page-user-list {\n  .user-list-filters {\n    .vs__actions {\n      position: absolute;\n      right: 0;\n      top: 50%;\n      transform: translateY(-58%);\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}