apitalize}}</vs-td>\n            <vs-td>\n              <vx-tooltip v-if=\"tr.paid\" :text=\"tr.paymentDate | formatDate\">\n                <p class=\"paymentstats p-made cursor-pointer\" color=\"success\">Payment Paid</p>\n              </vx-tooltip>\n              <p class=\"paymentstats p-pending\" v-else color=\"warning\">Pending</p>\n            </vs-td>\n            <vs-td>{{tr.paymentType |capitalize}}</vs-td>\n\n            <vs-td>\n              <a :href=\"`mailto:${tr.email}`\" target=\"_blank\">{{tr.email}}</a>\n            </vs-td>\n\n            <vs-td>\n              <vs-button\n                @click=\"viewRequest(tr)\"\n                color=\"primary\"\n                type=\"filled\"\n                size=\"small\"\n                class=\"mr-2 mb-2\"\n              >View</vs-button>\n              <vs-button\n                v-if=\"!tr.agreementGenerated\"\n                @click=\"viewGeneratePop(tr.name,tr._id)\"\n                color=\"primary\"\n                type=\"border\"\n                size=\"small\"\n                class=\"mr-2 mb-2\"\n              >Generate Report</vs-button>\n\n              <vs-button\n                v-if=\"tr.paymentType == 'bank'\"\n                @click=\"viewValidatePop(tr.name,tr._id)\"\n                color=\"primary\"\n                type=\"border\"\n                size=\"small\"\n                class=\"mr-2 mb-2\"\n              >Validate Payment</vs-button>\n            </vs-td>\n          </vs-tr>\n        </template>\n      </vs-table>\n    </vs-card>\n\n    <vs-popup\n      class=\"holamundo\"\n      :title=\"'Validate Payment for '+ ValidateData.churchName\"\n      :active.sync=\"validatePop\"\n    >\n      <div class=\"my-4\">\n        <div>\n          <div class=\"pb-3\">\n            <vs-input\n              name=\"amount\"\n              v-validate=\"'required'\"\n              v-model=\"amountValidate\"\n              label-placeholder=\"Enter Amount\"\n              class=\"w-full\"\n            />\n            <span\n              class=\"text-danger text-sm\"\n              v-show=\"errors.has('amount')\"\n            >{{ errors.first('amount') }}</span>\n          </div>\n          <vs-button @click.prevent=\"validatePaymentForm\" class=\"block mt-6 w-full\">Validate Payment</vs-button>\n        </div>\n      </div>\n    </vs-popup>\n\n    <vs-popup\n      class=\"holamundo\"\n      :title=\"'Generate Report for '+ reportChurch\"\n      :active.sync=\"generatePop\"\n    >\n      <div class=\"my-4\">\n        <form>\n          <div>\n            <div class=\"pb-3\">\n              <vs-input\n                name=\"setupCostNaira\"\n                v-validate=\"'required'\"\n                v-model=\"report.setupCostNaira\"\n                label-placeholder=\"Setup Cost in Naira\"\n                class=\"w-full\"\n              />\n              <span\n                class=\"text-danger text-sm\"\n                v-show=\"errors.has('setupCostNaira')\"\n              >{{ errors.first('setupCostNaira') }}</span>\n            </div>\n\n            <div class=\"pb-3\">\n              <vs-input\n                name=\"setupCostDollar\"\n                v-validate=\"'required'\"\n                v-model=\"report.setupCostDollar\"\n                label-placeholder=\"Setup Cost in Dollars\"\n                class=\"w-full\"\n              />\n              <span\n                class=\"text-danger text-sm\"\n                v-show=\"errors.has('setupCostDollar')\"\n              >{{ errors.first('setupCostDollar') }}</span>\n            </div>\n\n            <div class=\"pb-3\">\n              <vs-input\n                name=\"subcriptionCostNaira\"\n                v-validate=\"'required'\"\n                v-model=\"report.subcriptionCostNaira\"\n                label-placeholder=\"Subscription Cost in Naira\"\n                class=\"w-full\"\n              />\n              <span\n                class=\"text-danger text-sm\"\n                v-show=\"errors.has('subcriptionCostNaira')\"\n              >{{ errors.first('subcriptionCostNaira') }}</span>\n            </div>\n            <div class=\"pb-3\">\n              <vs-input\n                name=\"subcriptionCostDollar\"\n                v-validate=\"'required'\"\n                v-model=\"report.subcriptionCostDollar\"\n                label-placeholder=\"Subscription Cost in Dollars\"\n                class=\"w-full\"\n              />\n              <span\n                class=\"text-danger text-sm\"\n                v-show=\"errors.has('subcriptionCostDollar')\"\n              >{{ errors.first('subcriptionCostDollar') }}</span>\n            </div>\n\n            <vs-button @click.prevent=\"submitForm\" class=\"block mt-6 w-full\">Generate Report</vs-button>\n          </div>\n        </form>\n      </div>\n    </vs-popup>\n\n    <vs-popup class=\"holamundo\" :title=\"previewRequest.name\" :active.sync=\"viewRequestSign\">\n      <vs-list>\n        <vs-list-item title=\"Church Name\" :subtitle=\"previewRequest.name\"></vs-list-item>\n        <vs-list-item\n          title=\"Location\"\n          :subtitle=\"`${previewRequest.state}, ${previewRequest.city}`\"\n        ></vs-list-item>\n        <vs-list-item title=\"Address\" :subtitle=\"previewRequest.address\"></vs-list-item>\n        <vs-list-item title=\"Pricing Plan\" :subtitle=\"previewPricePlan | capitalize\"></vs-list-item>\n        <vs-list-item title=\"Email address\" :subtitle=\"previewRequest.email\"></vs-list-item>\n        <vs-list-header title=\"Contact Information\"></vs-list-header>\n        <vs-list-item\n          title=\"Contact Name\"\n          :subtitle=\"`${previewRequest.title} ${previewRequest.contactName}`\"\n        ></vs-list-item>\n        <vs-list-item title=\"Contact Phone\" :subtitle=\"previewRequest.contactPhoneNumber\"></vs-list-item>\n        <vs-list-header title=\"Other Information\"></vs-list-header>\n        <vs-list-item title=\"App Type\" :subtitle=\"previewRequest.type\"></vs-list-item>\n        <vs-list-item title=\"Postal Code\" :subtitle=\"previewRequest.postCode\"></vs-list-item>\n        <vs-list-item title=\"App Brand\" :subtitle=\"previewRequest.brand\"></vs-list-item>\n        <vs-list-item title=\"Mobile App Type\" v-if=\"previewRequest.android\" subtitle=\"Android\"></vs-list-item>\n        <vs-list-item title=\"Mobile App Type\" v-if=\"previewRequest.ios\" subtitle=\"IOS\"></vs-list-item>\n      </vs-list>\n      <div class=\"my-4\"></div>\n    </vs-popup>\n  </div>\n</template>\n\n<script>\nimport moment from \"moment\";\nimport vSelect from \"vue-select\";\nexport default {\n  components: {\n    vSelect,\n  },\n  data() {\n    return {\n      viewRequestSign: false,\n      generatePop: false,\n      validatePop: false,\n      blogs: [],\n      requests: [],\n      deleteID: \"\",\n      previewRequest: {},\n      previewPricePlan: \"\",\n      reportChurch: \"\",\n      reportID: \"\",\n      amountValidate: \"\",\n      ValidateData: {},\n      report: {\n        setupCostNaira: \"\",\n        setupCostDollar: \"\",\n        subcriptionCostNaira: \"\",\n        subcriptionCostDollar: \"\",\n      },\n    };\n  },\n  computed: {},\n  methods: {\n    openLoadingInDiv() {\n      this.$vs.loading({\n        container: \"#div-with-loading\",\n        scale: 0.6,\n      });\n    },\n    viewValidatePop(churchName, churchID) {\n      this.ValidateData = {\n        churchName: churchName,\n        churchID: churchID,\n      };\n      this.validatePop = true;\n    },\n    viewGeneratePop(churchName, churchID) {\n      this.reportChurch = churchName;\n      this.reportID = churchID;\n      this.generatePop = true;\n    },\n    getPricingPlan(plan) {\n      return plan.name;\n    },\n    getRequests() {\n      this.$store\n        .dispatch(\"getSignups\")\n        .then((resp) => {\n          // console.log(resp.data);`\n          this.requests = resp.data.request;\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n        })\n        .catch((err) => {\n          //        console.log(err);\n          if (err.response) {\n            if (err.response.status == 400) {\n              //   console.log(err.response.data);\n              this.$vs.notify({\n                title: \"Load Signup Requests\",\n                text: err.response.data,\n                color: \"danger\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Load Signup Requests\",\n                text: \"Unable to Load Signup Requests!\",\n                color: \"danger\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Load Signup Requests\",\n              text: \"Unable to Load Signup Requests!\",\n              color: \"danger\",\n            });\n          }\n\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n        });\n    },\n    viewRequest(request) {\n      this.viewRequestSign = true;\n      this.previewRequest = request;\n      this.previewPricePlan = request.pricingPlan.name;\n    },\n    validatePaymentForm() {\n      let validat = {\n        requestID: this.ValidateData.churchID,\n        amount: this.amountValidate,\n      };\n      this.openLoadingInDiv();\n\n      this.$store\n        .dispatch(\"validatePayment\", validat)\n        .then((resp) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n          this.$vs.notify({\n            title: \"Validate Payment\",\n            text: \"Payment validates Successfully\",\n            color: \"success\",\n          });\n          this.amountValidate = \"\";\n          this.validatePop = false;\n        })\n        .catch((err) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n          if (err.response) {\n            if (err.response.status == 400 || err.response.status === 403) {\n              this.$vs.notify({\n                title: \"Validate Payment\",\n                text: err.response.data,\n                color: \"warning\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Validate Payment\",\n                text: \"Unable to Validate Payment, try again!\",\n                color: \"warning\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Validate Payment\",\n              text: \"Unable to Validate Payment, try again!\",\n              color: \"warning\",\n            });\n          }\n        });\n\n      // this.$validator.validateAll().then((result) => {\n      //   if (result) {\n      //     this.openLoadingInDiv();\n      //     console.log(this.amountValidate);\n      //     // let validat = {\n      //     //   requestID: this.ValidateData.churchName,\n      //     //   amount: this.amountValidate,\n      //     // };\n      //     // console.log(validat);\n      //   } else {\n      //   }\n      // });\n    },\n    submitForm() {\n      this.openLoadingInDiv();\n      let reportData = {\n        churchid: this.reportID,\n        report: this.report,\n      };\n      this.$store\n        .dispatch(\"generateAgreement\", reportData)\n        .then((resp) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n          this.$vs.notify({\n            title: \"Generate Agreement\",\n            text: \"Agreement Generated Successfully\",\n            color: \"success\",\n          });\n          this.report.setupCostNaira = \"\";\n          this.report.setupCostDollar = \"\";\n          this.report.subcriptionCostNaira = \"\";\n          this.report.subcriptionCostDollar = \"\";\n          this.generatePop = false;\n        })\n        .catch((err) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n          if (err.response) {\n            if (err.response.status == 400 || err.response.status === 403) {\n              this.$vs.notify({\n                title: \"Generate Agreement\",\n                text: err.response.data,\n                color: \"warning\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Generate Agreement\",\n                text: \"Unable to generate agreement, try again!\",\n                color: \"warning\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Generate Agreement\",\n              text: \"Unable to generate agreement, try again!\",\n              color: \"warning\",\n            });\n          }\n        });\n\n      this.$validator.validateAll().then((result) => {\n        if (result) {\n          this.openLoadingInDiv();\n          let reportData = {\n            churchid: this.reportID,\n            report: this.report,\n          };\n          this.$store\n            .dispatch(\"generateAgreement\", reportData)\n            .then((resp) => {\n              this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n              this.$vs.notify({\n                title: \"Generate Agreement\",\n                text: \"Agreement Generated Successfully\",\n                color: \"success\",\n              });\n              this.report.setupCostNaira = \"\";\n              this.report.setupCostDollar = \"\";\n              this.report.subcriptionCostNaira = \"\";\n              this.report.subcriptionCostDollar = \"\";\n              this.generatePop = false;\n            })\n            .catch((err) => {\n              this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n              if (err.response) {\n                if (err.response.status == 400 || err.response.status === 403) {\n                  this.$vs.notify({\n                    title: \"Generate Agreement\",\n                    text: err.response.data,\n                    color: \"warning\",\n                  });\n                } else {\n                  this.$vs.notify({\n                    title: \"Generate Agreement\",\n                    text: \"Unable to generate agreement, try again!\",\n                    color: \"warning\",\n                  });\n                }\n              } else {\n                this.$vs.notify({\n                  title: \"Generate Agreement\",\n                  text: \"Unable to generate agreement, try again!\",\n                  color: \"warning\",\n                });\n              }\n            });\n        } else {\n        }\n      });\n    },\n  },\n  mounted() {\n    moment().format();\n    this.openLoadingInDiv();\n    this.getRequests();\n  },\n};\n</script>\n\n<style lang=\"scss\">\n#page-user-list {\n  .user-list-filters {\n    .vs__actions {\n      position: absolute;\n      right: 0;\n      top: 50%;\n      transform: translateY(-58%);\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}