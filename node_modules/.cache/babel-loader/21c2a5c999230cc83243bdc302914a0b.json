tting.trialStatus\"\n              >\n                <span slot=\"on\">Active</span>\n                <span slot=\"off\">Inactive</span>\n              </vs-switch>\n            </vs-list-item>\n\n            <vs-button type=\"filled\" @click.prevent=\"submitForm\" class=\"mt-5 block\">Save Settings</vs-button>\n          </div>\n        </div>\n      </div>\n    </vs-card>\n  </div>\n</template>\n\n<script>\nimport vSelect from \"vue-select\";\nexport default {\n  components: {\n    \"v-select\": vSelect,\n  },\n  data() {\n    return {\n      blog: {},\n      setting: {\n        trialStatus: false,\n        trialDays: \"\",\n        trialPromoCode: \"\",\n      },\n    };\n  },\n  mounted() {\n    this.getSettings();\n  },\n  methods: {\n    openLoadingInDiv() {\n      this.$vs.loading({\n        container: \"#div-with-loading\",\n        scale: 0.6,\n      });\n    },\n    getSettings() {\n      this.$store\n        .dispatch(\"getSettings\")\n        .then((resp) => {\n          this.setting.trialStatus = resp.data.trialStatus;\n          this.setting.trialDays = resp.data.trialDays;\n          this.setting.trialPromoCode = resp.data.trialPromoCode;\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n        })\n        .catch((err) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n\n          if (err.response) {\n            if (err.response.status == 400) {\n              //        console.log(err.response.data);\n              this.$vs.notify({\n                title: \"Get App Settings\",\n                text: err.response.data,\n                color: \"warning\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Get App Settings\",\n                text: \"Unable to Get App Settings, try again!\",\n                color: \"warning\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Get App Settings\",\n              text: \"Unable to Get App Settings, try again!\",\n              color: \"warning\",\n            });\n          }\n        });\n    },\n    submitForm() {\n      this.openLoadingInDiv();\n      let settingsN = this.setting;\n      this.$store\n        .dispatch(\"saveSettings\", settingsN)\n        .then((resp) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n          this.$vs.notify({\n            title: \"Save App Settings\",\n            text: \"App Settings successfully updated!\",\n            color: \"success\",\n          });\n        })\n        .catch((err) => {\n          this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n\n          if (err.response) {\n            if (err.response.status == 400) {\n              //        console.log(err.response.data);\n              this.$vs.notify({\n                title: \"Save App Settings\",\n                text: err.response.data,\n                color: \"warning\",\n              });\n            } else {\n              this.$vs.notify({\n                title: \"Save App Settings\",\n                text: \"Unable to Save App Settings, try again!\",\n                color: \"warning\",\n              });\n            }\n          } else {\n            this.$vs.notify({\n              title: \"Save App Settings\",\n              text: \"Unable to Save App Settings, try again!\",\n              color: \"warning\",\n            });\n          }\n        });\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}