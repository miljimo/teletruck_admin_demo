                v-model=\"name\"\n              />\n            </div>\n          </div>\n          <div class=\"col-md-3\">\n            <div class=\"text-right\">\n              <!-- <vs-button color=\"primary\" type=\"border\" class=\"mr-2\">\n                <vs-icon icon=\"visibility\" class=\"mr-1\"></vs-icon>\n                Preview</vs-button\n              > -->\n              <vs-button @click=\"saveContent\" color=\"primary\" type=\"filled\"\n                >Save</vs-button\n              >\n            </div>\n          </div>\n        </div>\n      </div>\n    </vs-card>\n    <div class=\"row\">\n      <div class=\"col-md-8\">\n        <vs-card class=\"mb-5\">\n          <div class=\"p-3\">\n            <div class=\"mb-10\">\n              <h4>Synopsis</h4>\n              <!-- synopsis -->\n              <quill-editor\n                class=\"mt-5 smEditor\"\n                v-model=\"synopsis\"\n                :options=\"editorOption\"\n              >\n                <div id=\"toolbar\" slot=\"toolbar\">\n                  <!-- Add a bold button -->\n                  <button class=\"ql-bold\">Bold</button>\n                  <button class=\"ql-italic\">Italic</button>\n\n                  <!-- Add font size dropdown -->\n                  <select class=\"ql-size\">\n                    <option value=\"small\" />\n                    <!-- Note a missing, thus falsy value, is used to reset to default -->\n                    <option selected />\n                    <option value=\"large\" />\n                    <option value=\"huge\" />\n                  </select>\n\n                  <select class=\"ql-font\">\n                    <option selected=\"selected\" />\n                    <option value=\"serif\" />\n                    <option value=\"monospace\" />\n                  </select>\n\n                  <!-- Add subscript and superscript buttons -->\n                  <button class=\"ql-script\" value=\"sub\" />\n                  <button class=\"ql-script\" value=\"super\" />\n\n                  <!-- You can also add your own -->\n                </div>\n              </quill-editor>\n            </div>\n\n            <div class=\"mb-5\">\n              <h4>Body</h4>\n              <!-- synopsis -->\n              <quill-editor class=\"mt-5 smEditor\" v-model=\"body\" />\n            </div>\n          </div>\n        </vs-card>\n      </div>\n      <div class=\"col-md-4\">\n        <vs-card>\n          <div class=\"p-3\">\n            <div class=\"mb-5\">\n              <p class=\"lead\">Child Month</p>\n\n              <vs-input\n                class=\"mb-4 w-full\"\n                placeholder=\"Month\"\n                type=\"number\"\n                v-model=\"child_month\"\n              />\n            </div>\n            <!-- <div class=\"mb-5\">\n              <p class=\"lead\">Album</p>\n              <vs-input class=\"w-full mt-2\" placeholder=\"\" />\n            </div> -->\n            <div class=\"mb-5\">\n              <p class=\"lead\">Maternal Status</p>\n\n              <vs-select class=\"mb-4 w-full\" v-model=\"maternal_id\">\n                <vs-select-item\n                  :key=\"index\"\n                  :value=\"item.id\"\n                  :text=\"item.name\"\n                  v-for=\"(item, index) in maternalStatus\"\n                />\n              </vs-select>\n            </div>\n            <div class=\"mb-5\">\n              <p class=\"lead\">Banner image</p>\n\n              <vs-input\n                @change=\"addImage($event)\"\n                class=\"w-full mt-2\"\n                placeholder=\"\"\n                type=\"file\"\n              />\n            </div>\n          </div>\n        </vs-card>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\n// eslint-disable-next-line\nimport \"quill/dist/quill.core.css\";\n// eslint-disable-next-line\nimport \"quill/dist/quill.snow.css\";\n// eslint-disable-next-line\nimport \"quill/dist/quill.bubble.css\";\nimport { quillEditor } from \"vue-quill-editor\";\nexport default {\n  components: {\n    quillEditor,\n  },\n  computed: {\n    loading() {\n      return this.$store.getters.pgLoading;\n    },\n  },\n  data() {\n    return {\n      name: \"\",\n      maternal_id: \"\",\n      child_month: \"\",\n      synopsis: \"\",\n      body: \"\",\n      image: \"\",\n      editorOption: {\n        modules: {\n          toolbar: \"#toolbar\",\n        },\n      },\n      newOption: {\n        modules: {\n          toolbar: \"#nbar\",\n        },\n      },\n      newOptionMother: {\n        modules: {\n          toolbar: \"#newOptionMother\",\n        },\n      },\n\n      editorOption2: {\n        modules: {\n          toolbar: \"#toolbar\",\n        },\n      },\n      synopsis: \"\",\n      body: \"\",\n      content: ``,\n      maternalStatus: [],\n    };\n  },\n  mounted() {\n    this.getStatus();\n  },\n  methods: {\n    getStatus() {\n      this.$store.commit(\"pgLoading\", true);\n      // this.$vs.loading();\n      this.$store\n        .dispatch(\"getMaternal\")\n        .then((resp) => {\n          this.maternalStatus = resp.data;\n          this.$store.commit(\"pgLoading\", false);\n        })\n        .catch((err) => {\n          this.$store.commit(\"pgLoading\", false);\n        });\n    },\n    getBase64(file) {\n      var reader = new FileReader();\n      reader.readAsDataURL(file);\n      reader.onload = () => {\n        this.image = reader.result;\n      };\n      reader.onerror = (error) => {\n        return false;\n      };\n    },\n    addImage(event) {\n      this.getBase64(event.target.files[0]);\n    },\n    saveContent() {\n      this.$vs.loading();\n      let content = {\n        path: \"child\",\n        data: {\n          name: this.name,\n          maternal_id: parseInt(this.maternal_id.toString()),\n          child_month: parseInt(this.child_month),\n          synopsis: this.synopsis,\n          body: this.body,\n          image: this.image,\n        },\n      };\n\n      this.$store\n        .dispatch(\"addContent\", content)\n        .then((resp) => {\n          this.$vs.loading.close();\n          this.$vs.notify({\n            title: \"Add Content\",\n            text: \"New Content added successful\",\n            color: \"success\",\n            icon: \"verified_user\",\n            position: \"bottom-center\",\n          });\n          // if (resp.data.status) {\n          //   this.$vs.notify({\n          //     title: \"Add Blog\",\n          //     text: \"New Blog added successful\",\n          //     color: \"success\",\n          //     icon: \"verified_user\",\n          //     position: \"bottom-center\",\n          //   });\n          // }\n          this.name = \"\";\n          this.maternal_id = \"\";\n          this.child_month = \"\";\n          this.synopsis = \"\";\n          this.body = \"\";\n          this.image = \"\";\n        })\n        .catch((err) => {\n          this.$vs.loading.close();\n          if (err.response) {\n            this.$vs.notify({\n              title: \"Add Content\",\n              text: err.response.data.message,\n              color: \"warning\",\n              icon: \"error\",\n              position: \"bottom-center\",\n            });\n          } else {\n            this.$vs.notify({\n              title: \"Add Content\",\n              text: \"Unable to add new content\",\n              color: \"dark\",\n              icon: \"error\",\n              position: \"bottom-center\",\n            });\n          }\n        });\n    },\n  },\n};\n</script>"],"sourceRoot":"src/views"}]}