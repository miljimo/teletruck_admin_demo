<vs-td>{{tr.phoneNumber}}</vs-td>\n            <vs-td :data=\"tr.churchId\">{{ getMemberChurch(tr.churchId) }}</vs-td>\n            <vs-td>\n              <vs-button\n                :to=\"'/view-member/'+ tr._id\"\n                color=\"primary\"\n                type=\"filled\"\n                size=\"small\"\n                class=\"mr-2 mb-2\"\n              >View</vs-button>\n              <!-- <vs-button color=\"dark\" size=\"small\">Change Status</vs-button> -->\n            </vs-td>\n          </vs-tr>\n        </template>\n      </vs-table>\n    </vs-card>\n  </div>\n</template>\n\n<script>\nimport moment from \"moment\";\nimport vSelect from \"vue-select\";\nexport default {\n  components: {\n    vSelect,\n  },\n  data() {\n    return {\n      members: [],\n    };\n  },\n  computed: {},\n  methods: {\n    openLoadingInDiv() {\n      this.$vs.loading({\n        container: \"#div-with-loading\",\n        scale: 0.6,\n      });\n    },\n    getMemberChurch(church) {\n      if (church) {\n        return church.name;\n      } else {\n        return \"\";\n      }\n    },\n  },\n  mounted() {\n    moment().format();\n    this.openLoadingInDiv();\n    this.$store\n      .dispatch(\"getAllMembers\")\n      .then((resp) => {\n        // console.log(resp.data);\n        this.members = resp.data;\n        this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n      })\n      .catch((err) => {\n        //        console.log(err);\n        if (err.response) {\n          if (err.response.status == 400) {\n            //        console.log(err.response.data);\n            this.$vs.notify({\n              title: \"Load Admin\",\n              text: err.response.data,\n              color: \"danger\",\n            });\n          } else {\n            this.$vs.notify({\n              title: \"Load Admin\",\n              text: \"Unable to Load list of Members!\",\n              color: \"danger\",\n            });\n          }\n        } else {\n          this.$vs.notify({\n            title: \"Load Admin\",\n            text: \"Unable to Load list of Members!\",\n            color: \"danger\",\n          });\n        }\n\n        this.$vs.loading.close(\"#div-with-loading > .con-vs-loading\");\n      });\n  },\n};\n</script>\n\n<style lang=\"scss\">\n#page-user-list {\n  .user-list-filters {\n    .vs__actions {\n      position: absolute;\n      right: 0;\n      top: 50%;\n      transform: translateY(-58%);\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}